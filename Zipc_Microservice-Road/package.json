{
  "name": "zipc_microservice-road",
  "version": "0.10.0",
  "description": "",
  "scripts": {
    "start": "nodemon --inspect src/index.ts",
    "build": "npm-run-all clean tsc opy-package-files",
    "build-product": "npm-run-all clean tsc copy-package-files copy-static-files",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "copy-package-files": "copyfiles package.json dist/Zipc_Microservice-Road",
    "copy-static-files": "copyfiles -u 2 -a public/static/**/*.* dist/Zipc_Microservice-Road/static/",
    "check-types": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^12.12.62",
    "@types/sparql-http-client": "^2.2.5",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "body-parser": "^1.19.0",
    "caching-map": "^1.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "6.11.0",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "influx": "^5.8.0",
    "lint-staged": "^10.5.2",
    "mongodb": "^3.6.2",
    "mongoose": "^5.12.3",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "sparql-http-client": "^2.2.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "dependencies": {}
}