<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
  "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to='zipc_webplatform'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  <inherits name="com.smartgwt.SmartGwt"/>
  <inherits name="com.smartclient.theme.tahoe.Tahoe"/>

  <!-- Other module inherits                                      -->
  <inherits name="com.zipc.garden.core.ZGCommand"/>
  <inherits name='com.zipc.garden.model.arc.ARCModel' />
  <inherits name='com.zipc.garden.model.bp.BPModel' />
  <inherits name='com.zipc.garden.model.bps.BPSModel' />
  <inherits name='com.zipc.garden.model.cb.CBModel' />
  <inherits name='com.zipc.garden.model.cgen.CGENModel' />
  <inherits name='com.zipc.garden.model.core.COREModel' />
  <inherits name='com.zipc.garden.model.csc.CSCModel' />
  <inherits name='com.zipc.garden.model.cscs.CSCSModel' />
  <inherits name='com.zipc.garden.model.fm.FMModel' />
  <inherits name='com.zipc.garden.model.fmc.FMCModel' />
  <inherits name='com.zipc.garden.model.fmcs.FMConstraintSyntax' />
  <inherits name='com.zipc.garden.model.fsm.FSMModel' />
  <inherits name='com.zipc.garden.model.lgen.LGENModel' />
  <inherits name='com.zipc.garden.model.lsc.LSCModel' />
  <inherits name='com.zipc.garden.model.scd.SCDModel' />
  <inherits name='com.zipc.garden.model.scs.SCSModel' />
  <inherits name='com.zipc.garden.model.spql.SPQLModel' />
  <inherits name='com.zipc.garden.model.tc.TCModel' />
  <inherits name='com.zipc.garden.model.tp.TPModel' />
  <inherits name='com.zipc.garden.model.tps.TPSModel' />
  <inherits name='com.zipc.garden.model.project.GPModel' />
  <inherits name='com.zipc.garden.model.cscs.CSCSModel' />
  <inherits name='com.zipc.garden.model.scenario.SCENARIOModel' />

  <!-- Specify the app entry point class.     -->                    
  <entry-point class='com.zipc.garden.webplatform.client.Zipc_Webplatform'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

  <!-- allow Super Dev Mode -->
  <add-linker name="xsiframe"/>
</module>
